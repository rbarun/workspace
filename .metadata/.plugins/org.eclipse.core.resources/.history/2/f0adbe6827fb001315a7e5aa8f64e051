import java.util.Scanner;
import java.lang.Math;

public class InverseTrigFunctions {

	public static void main(String[] args) {
		
		System.out.println("InverseTrigFunctions Program");
		System.out.println("Enter the Angle in Degrees: ");
		
		Scanner input = new Scanner(System.in);
		double degrees = input.nextDouble();
		
		System.out.println("Arcsin: " + Math.sin(degrees));
		System.out.println("Arccos: " + Math.cos(degrees));
		System.out.println("Arctan: " + Math.tan(degrees));
	}

}
