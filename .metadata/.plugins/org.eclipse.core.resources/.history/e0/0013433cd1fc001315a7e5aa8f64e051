public class EqualsString {

	public static void main(String[] args) {

//equals()
		String abc = "bobho";
		String def = "bobho";
		String ghi = "kdsjf";
		Boolean statement;
		
		if (abc.equals(def)){	
		System.out.println("abc and def Are Equal");
		
		}else{
			System.out.println("abc and def Are Not Equal.");
		}
		if (abc.equals(ghi)){
			System.out.println("abc and ghi Are Equal");
			
		}else{
			System.out.println();
			System.out.println("abc and ghi Are Not Equal.");
		}
		
		}
}

