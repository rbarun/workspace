import java.util.Scanner;
import java.lang.Math;

public class InverseTrigFunctions {

	public static void main(String[] args) {
		
		System.out.println("InverseTrigFunctions Program");
		System.out.println("Enter the Angle in Degrees: ");
		
		Scanner input = new Scanner(System.in);
		double degrees = input.nextDouble();
		double bob = Math.toDegrees(degrees);
		System.out.println(degrees);
		System.out.println("Arcsin: " + Math.asin(bob));
		System.out.println("Arccos: " + Math.acos(bob));
		System.out.println("Arctan: " + Math.atan(bob));
	}

}
